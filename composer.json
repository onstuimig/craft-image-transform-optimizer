{
    "name": "onstuimig/craft-image-transform-optimizer",
    "description": "Wrapper for image-optimizer for Craft image transforms",
    "type": "craft-plugin",
	"version": "1.0.0",
    "keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"onstuimig",
		"image",
		"transform",
		"optimizer"
	],
    "license": "MIT",
    "authors": [
		{
			"name": "Onstuimig",
			"homepage": "https://onstuimig.nl"
		}
	],
	"support": {
		"issues": "https://github.com/onstuimig/craft-image-transform-optimizer/issues?state=open",
		"source": "https://github.com/onstuimig/craft-image-transform-optimizer",
		"docs": "https://github.com/onstuimig/craft-image-transform-optimizer/blob/main/README.md"
	},
    "require": {
        "craftcms/cms": "^4.3.0",
        "spatie/image-optimizer": "^1.3.0"
    },
    "autoload": {
        "psr-4": {
            "onstuimig\\imagetransformoptimizer\\": "src/"
        }
    },
	"extra": {
		"name": "Image Transform Optimizer",
		"handle": "image-transform-optimizer",
		"class": "onstuimig\\imagetransformoptimizer\\ImageTransformOptimizer"
	}
}
